<Ui xmlns="http://www.runewaker.com/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.runewaker.com/ ../../worldxml/ui.xsd">

	<!-- Main Dialog!-->
	<Frame name="CPPimpDropDown" inherits="UIDropDownMenuTemplate" virtual="true">
		<Size>
			<AbsDimension x="40" y="25"/>
		</Size>
		<Layers>
			<Layer>
				<FontString name="$parentLabel" inherits="GameFontHighlight" justifyH="RIGHT">
					<Size>
						<AbsDimension x="79" y="16"></AbsDimension>
					</Size>
					<Anchors>
						<Anchor point="LEFT">
							<Offset>
								<AbsDimension x="-80" y="1"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>CP.Pimp.OnCtrlLoad(this)</OnLoad>
		</Scripts>
	</Frame>
	<Frame name="CPStatSelectionTemplate" virtual="true">
		<Size><AbsDimension x="160" y="26"></AbsDimension></Size>
		<Layers>
			<Layer>
				<FontString name="$parentLabel" inherits="GameFontHighlight"  justifyH="RIGHT">
					<Size><AbsDimension x="43" y="16"></AbsDimension></Size>
					<Anchors><Anchor point="LEFT"/></Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<EditBox name="$parentName" inherits="UIPanelEditBoxTemplate">
				<Size><AbsDimension x="100" y="22"></AbsDimension></Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parentLabel" relativePoint="RIGHT">
                        <Offset><AbsDimension x="1" y="0"/></Offset>
                    </Anchor>
				</Anchors>
				<Scripts>
                    <OnTabPressed>CP.Pimp.StatName_Tab(this)</OnTabPressed>
					<OnEditFocusLost>CP.Pimp.StatName_Finished(this)</OnEditFocusLost>
				</Scripts>
				<FontString inherits="GameFontNormalSmall" />
				<TextInsets><AbsInset top="6" right="3" left="4" bottom="0"></AbsInset></TextInsets>
			</EditBox>
			<Frame name="$parentTier" inherits="UIDropDownMenuTemplate">
				<Size><AbsDimension x="40" y="20"></AbsDimension></Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parentName" relativePoint="RIGHT"></Anchor>
				</Anchors>
			</Frame>
			<Button name="$parentSearch" inherits="UIPanelButtonTemplate" text="....">
				<Size><AbsDimension x="25" y="24"></AbsDimension></Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parentTier" relativePoint="RIGHT"></Anchor>
				</Anchors>
				<Scripts>
					<OnClick>CP.Pimp.OnStatSelSearch(this:GetParent())</OnClick>
				</Scripts>
			</Button>
		</Frames>
		<Scripts>
			<OnLoad>CP.Pimp.OnStatCtrlLoad(this)</OnLoad>
		</Scripts>
	</Frame>

	<Frame name="CPPimpMe" parent="CPFrame" inherits="UICommonFrameTemplate">
		<Size>
			<AbsDimension x="660" y="550"/>
		</Size>
		<Anchors>
			<Anchor point="TOPLEFT" relativePoint="TOP" >
				<Offset>
					<AbsDimension x="0" y="-60"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Frames>
			<GameTooltip name="$parentToolTip" inherits="GameTooltipTemplate" frameStrata="DIALOG" frameLevel="0">
				<Anchors>
					<Anchor point="TOPLEFT"><Offset>
						<AbsDimension x="9" y="30" />
					</Offset></Anchor>
					<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMLEFT">
						<Offset><AbsDimension x="400" y="-9" ></AbsDimension></Offset>
					</Anchor>
				</Anchors>
			</GameTooltip>
			<Frame name="CPPimpMeAttr">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset><AbsDimension x="415" y="36" /></Offset>
					</Anchor>
					<Anchor point="BOTTOMRIGHT">
						<Offset><AbsDimension x="-10" y="-10" /></Offset>
					</Anchor>
				</Anchors>
				<Layers>
					<Layer level="BACKGROUND">
						<Texture file="Interface\Common\PanelSeparate-HorizontalLine">
							<Size>
								<AbsDimension x="160" y="4"/>
							</Size>
							<Anchors>
								<Anchor point="TOP">
									<Offset>
										<AbsDimension x="2" y="98"/>
									</Offset>
								</Anchor>
							</Anchors>
						</Texture>
						<Texture file="Interface\Common\PanelSeparate-HorizontalLine">
							<Size>
								<AbsDimension x="160" y="4"/>
							</Size>
							<Anchors>
								<Anchor point="TOP">
									<Offset>
										<AbsDimension x="2" y="268"/>
									</Offset>
								</Anchor>
							</Anchors>
						</Texture>
						<FontString name="$parentDuraLabel" justifyH="RIGHT" inherits="GameFontHighlight">
							<Size><AbsDimension x="80" y="24"></AbsDimension></Size>
							<Anchors>
								<Anchor point="TOPLEFT">
									<Offset><AbsDimension x="20" y="70"/></Offset>
								</Anchor>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
				<Frames>
					<Button name="CPPimpMeItem" inherits="ItemButtonTemplate">
							<Anchors><Anchor point="TOPLEFT" /></Anchors>
							<Scripts>
								<OnClick>CP.Pimp.OnItemClicked()</OnClick>
							</Scripts>
					</Button>
					<Frame name="$parentPlus" inherits="CPPimpDropDown">
						<Anchors>
							<Anchor point="TOPLEFT"><Offset><AbsDimension x="100" y="8"/></Offset></Anchor>
						</Anchors>
					</Frame>
					<Frame name="$parentTier" inherits="CPPimpDropDown">
						<Anchors>
							<Anchor point="TOPLEFT" relativeTo="$parentPlus" relativePoint="BOTTOMLEFT">
								<Offset><AbsDimension x="0" y="6"/></Offset></Anchor>
						</Anchors>
					</Frame>
					<EditBox name="$parentDura" inherits="UIPanelEditBoxTemplate" numeric="true">
						<Size>
							<AbsDimension x="40" y="32"/>
						</Size>
						<Anchors>
							<Anchor point="TOPLEFT" relativeTo="$parentTier" relativePoint="BOTTOMLEFT"><Offset><AbsDimension x="5" y="3"/></Offset>
							</Anchor>
						</Anchors>
						<Scripts>
							<OnTextChanged>CP.Pimp.OnDura_Changed(this)</OnTextChanged>
						</Scripts>
						<TextInsets><AbsInset top="8" right="4" left="4" bottom="0"></AbsInset></TextInsets>
					</EditBox>
					<EditBox name="$parentMaxDura" inherits="UIPanelEditBoxTemplate" numeric="true">
						<Size>
							<AbsDimension x="40" y="32"/>
						</Size>
						<Anchors>
							<Anchor point="TOPLEFT" relativeTo="$parentDura" relativePoint="TOPRIGHT"><Offset><AbsDimension x="5" y="0"/></Offset>
							</Anchor>
						</Anchors>
						<Scripts>
							<OnTextChanged>CP.Pimp.OnMaxDura_Changed(this)</OnTextChanged>
						</Scripts>
						<TextInsets><AbsInset top="8" right="4" left="4" bottom="0"></AbsInset></TextInsets>
					</EditBox>
					<Frame name="$parentStat1" inherits="CPStatSelectionTemplate" id="1">
						<Anchors>
							<Anchor point="TOPLEFT"><Offset><AbsDimension x="0" y="107"/></Offset></Anchor>
						</Anchors>
					</Frame>
					<Frame name="$parentStat2" inherits="CPStatSelectionTemplate" id="2">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parentStat1" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parentStat3" inherits="CPStatSelectionTemplate" id="3">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parentStat2" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parentStat4" inherits="CPStatSelectionTemplate" id="4">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parentStat3" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parentStat5" inherits="CPStatSelectionTemplate" id="5">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parentStat4" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parentStat6" inherits="CPStatSelectionTemplate" id="6">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parentStat5" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parentRune1" inherits="CPStatSelectionTemplate" id="7">
						<Anchors>
							<Anchor point="TOPLEFT"><Offset><AbsDimension x="0" y="280"/></Offset></Anchor>
						</Anchors>
					</Frame>
					<Frame name="$parentRune2" inherits="CPStatSelectionTemplate" id="8">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parentRune1" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parentRune3" inherits="CPStatSelectionTemplate" id="9">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parentRune2" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parentRune4" inherits="CPStatSelectionTemplate" id="10">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parentRune3" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Button name="$parentBtnOK" inherits="UIPanelButtonTemplate" text="OK">
						<Size><AbsDimension x="60" y="24"></AbsDimension></Size>
						<Anchors>
							<Anchor point="BOTTOM"><Offset><AbsDimension x="-45" y="-10"></AbsDimension></Offset></Anchor>
						</Anchors>
						<Scripts>
							<OnClick>CP.Pimp.OnOK(this)</OnClick>
						</Scripts>
					</Button>
					<Button name="$parentBtnCancel" inherits="UIPanelButtonTemplate" text="CANCEL">
						<Size><AbsDimension x="60" y="24"></AbsDimension></Size>
						<Anchors>
							<Anchor point="BOTTOM"><Offset><AbsDimension x="55" y="-10"></AbsDimension></Offset></Anchor>
						</Anchors>
						<Scripts>
							<OnClick>CP.Pimp.OnCancel(this)</OnClick>
						</Scripts>
					</Button>
				</Frames>
			</Frame>
		</Frames>
		<Scripts>
			<OnShow>CP.Pimp.OnShow(this)</OnShow>
			<OnHide>CP.Pimp.OnHide(this)</OnHide>
		</Scripts>
	</Frame>

	<!-- Stat Search Dialog!-->
	<Frame name="CPStatSearchItemTemplate" enableMouse="true" virtual="true">
		<Size><AbsDimension x="265" y="15"></AbsDimension></Size>
		<Layers>
			<Layer>
				<FontString name="$parentName" inherits="GameFontHighlight" justifyH="LEFT">
					<Size>
						<AbsDimension x="90" y="14"></AbsDimension>
					</Size>
					<FontHeight><AbsValue val="12"/></FontHeight>
				</FontString>
				<FontString name="$parentValue" inherits="GameFontNormalSmall"  justifyH="LEFT">
					<Size><AbsDimension x="155" y="14"></AbsDimension></Size>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parentName" relativePoint="RIGHT"></Anchor>
					</Anchors>
					<FontHeight><AbsValue val="9"/></FontHeight>
				</FontString>
				<Texture name="$parentHighlight" file="Interface\AuctionFrame\AuctionFrameItem-Highlight" alphaMode="ADD" hidden="true">
				<Anchors>
					<Anchor point="TOPLEFT"><Offset><AbsDimension x="-2" y="0"></AbsDimension></Offset></Anchor>
					<Anchor point="BOTTOMRIGHT"><Offset><AbsDimension x="-10" y="-1"></AbsDimension></Offset></Anchor>
				</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnClick>CP.Pimp.StatSearch_ItemClicked(this,key)</OnClick>
			<OnDoubleClick>
					CP.Pimp.StatSearch_ItemClicked(this,key)
					CP.Pimp.StatSearch_Close()
			</OnDoubleClick>
			<OnMouseWheel>
				CP.Pimp.StatSearch_ItemOnLeave()
				UIPanelScrollBar_OnMouseWheel(CPStatSearchItemSB, delta)
				CP.Pimp.StatSearch_ItemOnEnter(this)
			</OnMouseWheel>
			<OnEnter>CP.Pimp.StatSearch_ItemOnEnter(this)</OnEnter>
			<OnLeave>CP.Pimp.StatSearch_ItemOnLeave()</OnLeave>
		</Scripts>
	</Frame>
	<Frame name="CPStatSearch" hidden="true" parent="CPPimpMe" inherits="UICommonFrameTemplate" frameStrata="TOOLTIP" frameLevel="1000">
		<Size><AbsDimension x="290" y="300"></AbsDimension></Size>
		<Frames>
		     <EditBox name="$parentSearchBox" inherits="UIPanelEditBoxTemplate">
				<Size><AbsDimension x="150" y="20"/></Size>
				<Anchors>
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT"><Offset><AbsDimension x="50" y="29" /></Offset></Anchor>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<FontString name="$parentBack" inherits="GameFontDisable">
							<Anchors>
								<Anchor point="LEFT"><Offset><AbsDimension x="5" y="0"></AbsDimension></Offset></Anchor>
							</Anchors>
							<FontHeight><AbsValue val="12"></AbsValue></FontHeight>
						</FontString>
					</Layer>
				</Layers>
				<Scripts>
					<OnEditFocusGained>CP.Pimp.StatSearch_FilterFocus(this,true)</OnEditFocusGained>
					<OnEditFocusLost>CP.Pimp.StatSearch_FilterFocus(this,false)</OnEditFocusLost>
					<OnTextChanged>CP.Pimp.StatSearch_UpdateList(this)</OnTextChanged>
				</Scripts>
			</EditBox>
			<Frame name="$parentItem" enableMouse="true">
				<Size>
					<AbsDimension x="270" y="210"></AbsDimension>
				</Size>
				<Anchors>
					<Anchor point="TOP"><Offset><AbsDimension y="50" x="0"></AbsDimension></Offset></Anchor>
				</Anchors>
				<Backdrop edgeFile="Interface\Tooltips\Tooltip-border">
				    <EdgeSize><AbsValue val="8"/></EdgeSize>
				</Backdrop>
				<Frames>
					<Frame name="$parent1" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT"><Offset><AbsDimension x="4" y="8"></AbsDimension></Offset></Anchor>
						</Anchors>
					</Frame>
					<Frame name="$parent2" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parent1" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parent3" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parent2" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parent4" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parent3" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parent5" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parent4" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parent6" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parent5" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parent7" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parent6" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parent8" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parent7" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parent9" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parent8" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parent10" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parent9" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parent11" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parent10" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parent12" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parent11" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Frame name="$parent13" inherits="CPStatSearchItemTemplate">
						<Anchors><Anchor point="TOPLEFT" relativeTo="$parent12" relativePoint="BOTTOMLEFT" /></Anchors>
					</Frame>
					<Slider name="$parentSB" inherits="UIPanelScrollBarTemplate">
						<Anchors>
							<Anchor point="TOPRIGHT" relativePoint="TOPRIGHT"><Offset><AbsDimension x="-1" y="19"/></Offset></Anchor>
							<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"><Offset><AbsDimension x="-1" y="-19"/></Offset></Anchor>
						</Anchors>
						<Scripts>
							<OnValueChanged>
								UIPanelScrollBar_OnValueChanged(this, arg1)
								CP.Pimp.StatSearch_ListUpdate()
							</OnValueChanged>
						</Scripts>
					</Slider>
				</Frames>
				<Scripts>
					<OnMouseWheel>UIPanelScrollBar_OnMouseWheel(CPStatSearchItemSB, delta)</OnMouseWheel>
				</Scripts>
			</Frame>
			<Button name="$parentBtnOK" inherits="UIPanelButtonTemplate" text="OK">
				<Size><AbsDimension x="60" y="24"></AbsDimension></Size>
				<Anchors>
					<Anchor point="BOTTOM"><Offset><AbsDimension x="-45" y="-8"></AbsDimension></Offset></Anchor>
				</Anchors>
				<Scripts>
					<OnClick>CP.Pimp.StatSearch_Close()</OnClick>
				</Scripts>
			</Button>
			<Button name="$parentBtnCancel" inherits="UIPanelButtonTemplate" text="CANCEL">
				<Size><AbsDimension x="60" y="24"></AbsDimension></Size>
				<Anchors>
					<Anchor point="BOTTOM"><Offset><AbsDimension x="55" y="-8"></AbsDimension></Offset></Anchor>
				</Anchors>
				<Scripts>
					<OnClick>CP.Pimp.StatSearch_Cancel()</OnClick>
				</Scripts>
			</Button>
		</Frames>
		<Scripts>
			<OnShow>CP.Pimp.StatSearch_OnShow(this)</OnShow>
			<OnHide>CP.Pimp.StatSearch_OnHide(this)</OnHide>
		</Scripts>

	</Frame>
</Ui>
