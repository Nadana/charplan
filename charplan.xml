					<Anchors>
						<Anchor point="BOTTOM">
							<Offset><AbsDimension x="0" y="0"/></Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>CP.AttributeTitle_OnLoad( this )</OnLoad>
		</Scripts>
	</Frame>
