	<!-- Equipment Buttons !-->
	<Button name="CPEquipItemButtonTemplate" inherits="ItemButtonTemplate" virtual="true">
		<Scripts>
			<OnClick>CP.EquipItem_OnClick(this, key)</OnClick>
			<OnLoad>CP.EquipItem_OnLoad( this )</OnLoad>
			<OnEnter>CP.EquipItem_OnEnter( this )</OnEnter>
			<OnLeave>CP.EquipItem_OnLeave( this )</OnLeave>
			<OnReceiveDrag>CP.EquipItem_OnReceiveDrag( this )</OnReceiveDrag>
		</Scripts>
	</Button>
	<!-- Attribute Displays !-->
	<Frame name="CPAttributeFrameTemplate" enableMouse="true" virtual="true">
		<Size>
			<AbsDimension x="136" y="14"/>
		</Size>
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="$parentLabel" inherits="GameFontLabel" justifyH="LEFT">
					<Size>
						<AbsDimension x="96" y="14"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT">
							<Offset>
								<AbsDimension x="2" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parentValue" inherits="NumberFontNormal" justifyH="RIGHT">
					<Anchors>
						<Anchor point="RIGHT">
							<Offset>
								<AbsDimension x="-4" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>CP.Attribute_OnLoad( this )</OnLoad>
			<OnEnter>CP.Attribute_OnEnter(this)</OnEnter>
			<OnLeave>GameTooltip:Hide()</OnLeave>
		</Scripts>
	</Frame>
	<!-- Attribute Title display !-->
	<Frame name="CPAttributeTitleTemplate" virtual="true">
