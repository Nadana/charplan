				</Anchors>
			</Button>
			<CheckButton name="$parentShowCheckButton" hidden="true">
				<Size>
					<AbsDimension x="24" y="24"/>
				</Size>
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset>
							<AbsDimension x="2" y="-2"/>
						</Offset>
					</Anchor>
				</Anchors>
				<HitRectInsets>
					<AbsInset top="0" left="8" bottom="8" right="0"/>
				</HitRectInsets>
				<Scripts>
					<OnClick>
						EquipItemShowCheckButton_OnClick(this);
					</OnClick>
				</Scripts>
				<NormalTexture file="Interface\Buttons\CheckBox-Normal2"/>
				<PushedTexture file="Interface\Buttons\CheckBox-Depress2"/>
				<HighlightTexture file="Interface\Buttons\CheckBox-Highlight" alphaMode="ADD"/>
				<CheckedTexture file="Interface\Buttons\CheckBox-Checked"/>
				<DisabledCheckedTexture file="Interface\Buttons\CheckBox-Checked-Disabled"/>
			</CheckButton>
		</Frames>
		<Scripts>
			<OnLoad>
				--EquipItemButton_OnLoad(this);
			</OnLoad>
			<OnShow>
				--EquipItemButton_OnShow(this);
			</OnShow>
			<OnEvent>
				EquipItemButton_OnEvent(this, event, arg1, arg2);
			</OnEvent>
			<OnClick>
				EquipItemButton_OnClick(this, key);
			</OnClick>
			<OnDragStart>
				EquipItemButton_OnClick(this, "LBUTTON", 1);
			</OnDragStart>
			<OnReceiveDrag>
				EquipItemButton_OnClick(this, "LBUTTON", 1);
			</OnReceiveDrag>
			<!--
			<OnUpdate>
				EquipmentItemSlotButton_Update(this, elapsedTime);
			</OnUpdate>
			-->
			<OnEnter>
				EquipItemButton_OnEnter( this );
			</OnEnter>
			<OnLeave>
				EquipItemButton_OnLeave( this );
			</OnLeave>
		</Scripts>
	</Button>


