<Ui xmlns="http://www.runewaker.com/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.runewaker.com/ ../../worldxml/ui.xsd">
	<Frame name="CPSearchItemTemplate" enableMouse="true" virtual="true">
		<Size>
			<AbsDimension x="550" y="34" />
		</Size>
		<Layers>
			<Layer>
			<FontString name="$parentName" inherits="GameFontLabel" justifyH="LEFT">
				<Size>
					<AbsDimension x="180" y="14"/>
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT"><Offset><AbsDimension x="40" y="0"/></Offset></Anchor>
				</Anchors>
				<FontHeight><AbsValue val="12"></AbsValue></FontHeight>
			</FontString>
			<FontString name="$parentSet" inherits="GameFontNormal" justifyH="LEFT">
					<Size>
						<AbsDimension x="96" y="14"/>
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT" relativeTo="$parentName" relativePoint="BOTTOMLEFT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				<FontHeight><AbsValue val="10"></AbsValue></FontHeight>
				<Color r="0.6" g="0.4" b="0.2" />
				</FontString>
			<FontString name="$parentLevel" inherits="GameFontLabel" justifyH="CENTER">
				<Size>
					<AbsDimension x="20" y="14"/>
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parentName" relativePoint="TOPRIGHT">
						<Offset><AbsDimension y="0" x="2"></AbsDimension></Offset>
					</Anchor>
				</Anchors>
				<FontHeight><AbsValue val="12"></AbsValue></FontHeight>
			</FontString>
			<FontString name="$parentEffect" inherits="GameFontNormal" justifyH="LEFT">
				<Size><AbsDimension x="100" y="34"/></Size>
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parentName" relativePoint="TOPRIGHT">
						<Offset><AbsDimension x="24" y="0" /></Offset>
					</Anchor>
				</Anchors>
				<FontHeight><AbsValue val="10"></AbsValue></FontHeight>
			</FontString>
			<FontString name="$parentBoni" inherits="GameFontGreen" justifyH="LEFT">
				<Size>
					<AbsDimension x="180" y="34"/>
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parentEffect" relativePoint="TOPRIGHT" />
				</Anchors>
				<FontHeight><AbsValue val="10"></AbsValue></FontHeight>
			</FontString>
			</Layer>
			<Layer level="BACKGROUND">
			<Texture name="$parentHighlight" file="Interface\AuctionFrame\AuctionFrameItem-Highlight" alphaMode="ADD" hidden="true">
				<Anchors>
					<Anchor point="TOPLEFT"><Offset><AbsDimension x="-2" y="0"></AbsDimension></Offset></Anchor>
					<Anchor point="BOTTOMRIGHT"><Offset><AbsDimension x="-10" y="-1"></AbsDimension></Offset></Anchor>
				</Anchors>
			</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Button name="$parentItem" inherits="ItemButtonTemplate">
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset><AbsDimension x="0" y="0" /></Offset>
						</Anchor>
					</Anchors>
				<Scripts>
					<OnEnter>CP.Search.OnItemEnter(this:GetParent())</OnEnter>
					<OnLeave>CP.Search.OnItemLeave(this:GetParent())</OnLeave>
				</Scripts>
			</Button>
		</Frames>
		<Scripts>
			<OnClick>CP.Search.OnItemClick(this)</OnClick>
			<OnDoubleClick>CP.Search.OnItemDblClick(this)</OnDoubleClick>
			<OnMouseWheel>
				--CP.Pimp.StatSearch_ItemOnLeave()
				UIPanelScrollBar_OnMouseWheel(CPSearchItemsSB, delta)
				--CP.Pimp.StatSearch_ItemOnEnter(this)
			</OnMouseWheel>
		</Scripts>
	</Frame>

	<Button name="CPSearchHeaderButtonTemplate" virtual="true">
		<Size>
			<AbsDimension x="64" y="20"/>
		</Size>
		<Layers>
			<Layer>
				<Texture name="$parentSortIcon">
					<Size>
						<AbsDimension x="8" y="8"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativePoint="RIGHT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnClick>CP.Search.HeaderClicked(this)</OnClick>
		</Scripts>
		<NormalText inherits="GameFontYellow"/>
		<PushedTextOffset>
			<AbsDimension x="1" y="1"/>
		</PushedTextOffset>
	</Button>

	<Frame name="CPSearch" parent="CPFrame"  inherits="UICommonFrameTemplate">
		<Size>
			<AbsDimension x="600" y="500"/>
		</Size>
		<Anchors>
			<Anchor point="TOPLEFT" relativePoint="TOP" >
				<Offset>
					<AbsDimension x="0" y="-60"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<FontString name="$parentFilterNameLabel" inherits="GameFontLabel"><Anchors>
					<Anchor point="TOPLEFT"><Offset><AbsDimension x="14" y="29"></AbsDimension></Offset></Anchor>
				</Anchors></FontString>
				<FontString name="$parentFilterLevelLabel" inherits="GameFontLabel"><Anchors>
					<Anchor point="TOPLEFT"><Offset><AbsDimension x="14" y="52"></AbsDimension></Offset></Anchor>
				</Anchors></FontString>
				<FontString inherits="GameFontLabel" text="-"><Anchors>
					<Anchor point="TOPLEFT"><Offset><AbsDimension x="103" y="52"></AbsDimension></Offset></Anchor>
				</Anchors></FontString>
			</Layer>
		</Layers>
		<Frames>
			<!-- Filter stuff !-->
		    <EditBox name="$parentFilterName" inherits="UIPanelEditBoxTemplate">
				<Size><AbsDimension x="120" y="20"/></Size>
				<Anchors>
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT"><Offset><AbsDimension x="54" y="28" /></Offset></Anchor>
				</Anchors>
				<Scripts>
					<OnTextChanged>CP.Search.FindItems()</OnTextChanged>
				</Scripts>
			</EditBox>
		    <EditBox name="$parentFilterLevelMin" inherits="UIPanelEditBoxTemplate" numeric="true">
				<Size><AbsDimension x="40" y="20"/></Size>
				<Anchors>
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT"><Offset><AbsDimension x="60" y="48" /></Offset></Anchor>
				</Anchors>
				<Scripts>
					<OnTextChanged>CP.Search.FindItems()</OnTextChanged>
				</Scripts>
			</EditBox>
		    <EditBox name="$parentFilterLevelMax" inherits="UIPanelEditBoxTemplate" numeric="true">
				<Size><AbsDimension x="40" y="20"/></Size>
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parentFilterLevelMin" relativePoint="TOPRIGHT"><Offset><AbsDimension x="12" y="0" /></Offset></Anchor>
				</Anchors>
				<Scripts>
					<OnTextChanged>CP.Search.FindItems()</OnTextChanged>
				</Scripts>
			</EditBox>
			<CheckButton name="$parentFilterStatLess" inherits="UIPanelCheckButtonTemplate" autoChecked="true">
				<Size>
					<AbsDimension x="18" y="20"/>
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="200" y="28"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Layers>
					<Layer>
						<FontString name="$parentText" inherits="GameFontHighlight" justifyH="LEFT">
							<Size>
								<AbsDimension x="90" y="16"/>
							</Size>
							<Anchors>
								<Anchor point="LEFT" relativePoint="RIGHT">
									<Offset>
										<AbsDimension x="0" y="0"/>
									</Offset>
								</Anchor>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
				<Scripts>
					<OnClick>CP.Search.FindItems()</OnClick>
				</Scripts>
			</CheckButton>
			<Button name="$parentFilterType" inherits="UIPanelButtonTemplate">
				<Size>
					<AbsDimension x="132" y="24"/>
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="200" y="47"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>CP.Search.ShowFilterMenu(this,"Type")</OnClick>
				</Scripts>
			</Button>
			<Frame name="CPSearchFilterMenu" inherits="UIDropDownMenuTemplate">
				<Size>
					<AbsDimension x="132" y="26"/>
				</Size>
				<Anchors>
					<Anchor point="TOP" relativeTo="$parentFilterType"/>
				</Anchors>
				<Scripts>
					<OnLoad>CP.Search.OnTypeFilterLoad(this)</OnLoad>
				</Scripts>
			</Frame>
			<Button name="$parentFilterSlot" inherits="UIPanelButtonTemplate">
				<Size>
					<AbsDimension x="132" y="24"/>
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="350" y="47"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>CP.Search.ShowFilterMenu(this,"Slot")</OnClick>
				</Scripts>
			</Button>
			<!-- item list !-->
			<Frame name="CPSearchItems">
				<Size>
					<AbsDimension x="580"  y="368"></AbsDimension>
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT"><Offset><AbsDimension x="10" y="70" /></Offset></Anchor>
				</Anchors>
				<Backdrop edgeFile="Interface\Tooltips\Tooltip-border">
				    <EdgeSize><AbsValue val="8"/></EdgeSize>
				</Backdrop>
				<Frames>
					<Button name="CPSearchHead1" inherits="CPSearchHeaderButtonTemplate" id="1">
						<Size><AbsDimension x="100" y="20"></AbsDimension></Size>
						<Anchors>
							<Anchor point="TOPLEFT"><Offset><AbsDimension x="40" y="1"/></Offset></Anchor>
						</Anchors>
					</Button>
					<Button name="CPSearchHead2" inherits="CPSearchHeaderButtonTemplate" id="2">
						<Size><AbsDimension x="50" y="20"></AbsDimension></Size>
						<Anchors>
							<Anchor point="TOPLEFT"><Offset><AbsDimension x="220" y="1"/></Offset></Anchor>
						</Anchors>
					</Button>
					<Button name="CPSearchHead3" inherits="CPSearchHeaderButtonTemplate" id="3">
						<Size><AbsDimension x="50" y="20"></AbsDimension></Size>
						<Anchors>
							<Anchor point="TOPLEFT"><Offset><AbsDimension x="260" y="1"/></Offset></Anchor>
						</Anchors>
					</Button>
					<Frame name="CPSearchItem1" inherits="CPSearchItemTemplate" id="1">
						<Anchors>
							<Anchor point="TOPLEFT"><Offset><AbsDimension x="2" y="22" /></Offset></Anchor>
						</Anchors>
					</Frame>
					<Frame name="CPSearchItem2" inherits="CPSearchItemTemplate" id="2">
						<Anchors><Anchor point="TOPLEFT" relativeTo="CPSearchItem1" relativePoint="BOTTOMLEFT"/></Anchors>
					</Frame>
					<Frame name="CPSearchItem3" inherits="CPSearchItemTemplate" id="3">
						<Anchors><Anchor point="TOPLEFT" relativeTo="CPSearchItem2" relativePoint="BOTTOMLEFT"/></Anchors>
					</Frame>
					<Frame name="CPSearchItem4" inherits="CPSearchItemTemplate" id="4">
						<Anchors><Anchor point="TOPLEFT" relativeTo="CPSearchItem3" relativePoint="BOTTOMLEFT"/></Anchors>
					</Frame>
					<Frame name="CPSearchItem5" inherits="CPSearchItemTemplate" id="5">
						<Anchors><Anchor point="TOPLEFT" relativeTo="CPSearchItem4" relativePoint="BOTTOMLEFT"/></Anchors>
					</Frame>
					<Frame name="CPSearchItem6" inherits="CPSearchItemTemplate" id="6">
						<Anchors><Anchor point="TOPLEFT" relativeTo="CPSearchItem5" relativePoint="BOTTOMLEFT"/></Anchors>
					</Frame>
					<Frame name="CPSearchItem7" inherits="CPSearchItemTemplate" id="7">
						<Anchors><Anchor point="TOPLEFT" relativeTo="CPSearchItem6" relativePoint="BOTTOMLEFT"/></Anchors>
					</Frame>
					<Frame name="CPSearchItem8" inherits="CPSearchItemTemplate" id="8">
						<Anchors><Anchor point="TOPLEFT" relativeTo="CPSearchItem7" relativePoint="BOTTOMLEFT"/></Anchors>
					</Frame>
					<Frame name="CPSearchItem9" inherits="CPSearchItemTemplate" id="9">
						<Anchors><Anchor point="TOPLEFT" relativeTo="CPSearchItem8" relativePoint="BOTTOMLEFT"/></Anchors>
					</Frame>
					<Frame name="CPSearchItem10" inherits="CPSearchItemTemplate" id="10">
						<Anchors><Anchor point="TOPLEFT" relativeTo="CPSearchItem9" relativePoint="BOTTOMLEFT"/></Anchors>
					</Frame>
					<Slider name="$parentSB" inherits="UIPanelScrollBarTemplate">
						<Anchors>
							<Anchor point="TOPRIGHT" relativePoint="TOPRIGHT"><Offset><AbsDimension x="-1" y="19"/></Offset></Anchor>
							<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"><Offset><AbsDimension x="-1" y="-19"/></Offset></Anchor>
						</Anchors>
						<Scripts>
							<OnValueChanged>
								UIPanelScrollBar_OnValueChanged(this, arg1)
								CP.Search.UpdateList()
							</OnValueChanged>
						</Scripts>
					</Slider>
				</Frames>
			</Frame>
			<Button name="CPSearchTakeIt" inherits="UIPanelButtonTemplate" text="OK">
				<Size>
					<AbsDimension x="100" y="24"></AbsDimension>
				</Size>
				<Anchors>
					<Anchor point="BOTTOM"><Offset>
						<AbsDimension x="-100" y="-20"></AbsDimension>
					</Offset></Anchor>
				</Anchors>
				<Scripts>
					<OnClick>CP.Search.OnTakeIt()</OnClick>
				</Scripts>
			</Button>
			<Button name="CPSearchCancel" inherits="UIPanelButtonTemplate" text="CANCEL">
				<Size>
					<AbsDimension x="100" y="24"></AbsDimension>
				</Size>
				<Anchors>
					<Anchor point="BOTTOM"><Offset>
						<AbsDimension x="100" y="-20"></AbsDimension>
					</Offset></Anchor>
				</Anchors>
				<Scripts>
					<OnClick>CP.Search.OnCancel()</OnClick>
				</Scripts>
			</Button>
		</Frames>
		<Scripts>
			<OnLoad>CP.Search.OnLoad(this)</OnLoad>
			<OnShow>CP.Search.OnShow(this)</OnShow>
			<OnHide>CP.Search.OnHide()</OnHide>
		</Scripts>
	</Frame>
</Ui>
